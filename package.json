{
  "name": "node-bcc-native",
  "version": "0.0.4",
  "description": "Native (Rust) implementation of beyerleinf/node-bcc",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyerleinf/node-bcc-native.git"
  },
  "bugs": {
    "url": "https://github.com/beyerleinf/node-bcc-native/issues"
  },
  "files": [
    "/lib",
    "/native/index.node"
  ],
  "author": "Fabian Beyerlein <mail@fabian-b.de>",
  "license": "MIT",
  "dependencies": {
    "@amilajack/node-pre-gyp": "github:amilajack/node-pre-gyp#neon-compat",
    "neon-cli": "^0.2.0",
    "node-pre-gyp-github": "^1.4.3",
    "snyk": "^1.316.1"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false || neon build --release",
    "package": "node-pre-gyp package",
    "upload-binary": "node-pre-gyp package && node-pre-gyp-github publish",
    "build": "neon build --release",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "devDependencies": {},
  "binary": {
    "module_name": "index",
    "host": "https://github.com/beyerleinf/node-bcc-native/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native",
    "pkg_path": "."
  },
  "snyk": true
}